        android:label="Konverter Satuan"
        android:roundIcon="@mipmap/ic_launcher_baru_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LatihanOne">
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:exported="true">
        </activity>
        <activity android:name=".DetailSuhuActivity"/>
        <activity android:name=".DetailMasaActivity"/>
        <activity android:name=".DetailLuasActivity"/>
        <activity android:name=".DetailJarakActivity"/>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>